proposal MultiStmt{
    case Smthn{
        applicable to{
            "let <<ident1:Identifier>> = <<funcIdent:Identifier | MemberExpression>>();
            let <<ident2:Identifier>> = <<expr:Expression>>;
            "
        }

        transform to {
            "const ident2 = () => {
                let <<ident1>> = <<funcIdent>>();
                return <<expr>>;
            }"
        }
    }
}